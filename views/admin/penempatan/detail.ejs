<%- include ../../partials/header.ejs %>
<%- include ../../partials/navbar.ejs %>
<%- include ../../partials/sidebar.ejs %>

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
  <!-- Content Header (Page header) -->
  <section class="content-header">
    <div class="container-fluid">
      <div class="row mb-2">
        <div class="col-sm-6">
          <h1>Penempatan Peserta Magang</h1>
        </div>
        <div class="col-sm-6">
          <ol class="breadcrumb float-sm-right">
            <li class="breadcrumb-item"><a href="/">Dashboard</a></li>
            <li class="breadcrumb-item"><a href="/penempatan">Penempatan</a></li>
            <li class="breadcrumb-item active">Detail</li>
          </ol>
        </div>
      </div>
    </div><!-- /.container-fluid -->
  </section>

  <!-- Main content -->
  <section class="content">
    <div class="container-fluid">
      <div class="row">
        <div class="col-12">
          <!-- /.card-header -->
          <div class="card-body">
            <!-- general form elements -->
            <div class="card card-primary">
              <div class="card-header">
                <h3 class="card-title">Detail Penempatan Peserta Magang</h3>
              </div>
              <!-- /.card-header -->
              <!-- form start -->
                <div class="card-body">
                  <div class="form-group">
                    <label for="exampleInputBorder">Nama peserta magang :</label>
                    <input type="text" class="form-control form-control-border" 
                    id="exampleInputBorder" value="<%= penempatan.peserta.name %> " disabled>
                  </div>
                  <div class="form-group">
                    <label for="exampleInputBorder">No.Induk peserta magang :</label>
                    <input type="text" class="form-control form-control-border" 
                    id="exampleInputBorder" value="<%= penempatan.peserta.nim %> " disabled>
                  </div>
                  <div class="form-group">
                    <label for="exampleInputBorder">Instansi/asal sekolah peserta magang :</label>
                    <input type="text" class="form-control form-control-border" 
                    id="exampleInputBorder" value="<%= penempatan.peserta.instansi %> " disabled>
                  </div>
                  <div class="form-group">
                    <label for="exampleInputBorder">Jurusan peserta magang :</label>
                    <input type="text" class="form-control form-control-border" 
                    id="exampleInputBorder" value="<%= penempatan.peserta.jurusan %> " disabled>
                  </div>
                  <div class="row">
                    <div class="col">
                      <div class="form-group">
                        <label for="exampleInputBorder">Tanggal mulai magang :</label>
                        <input type="text" class="form-control form-control-border" 
                        id="exampleInputBorder" value="<%= tglFormatForm(penempatan.peserta.tglmulai) %>" disabled>
                      </div>
                    </div>
                    <div class="col">
                      <div class="form-group">
                        <label for="exampleInputBorder">Tanggal selesai magang :</label>
                        <input type="text" class="form-control form-control-border" 
                        id="exampleInputBorder" value="<%= tglFormatForm(penempatan.peserta.tglselesai) %>" disabled>
                      </div>
                    </div>
                  </div>
                  <hr>
                  <div class="form-group">
                    <label for="exampleInputBorder">Nama Pembimbing lapangan (Supervisor) :</label>
                    <input type="text" class="form-control form-control-border" 
                    id="exampleInputBorder" value="<%= penempatan.supervisor.nama %>" disabled>
                  </div>
                  <div class="form-group">
                    <label for="exampleInputBorder">No. Induk Pembimbing lapangan (Supervisor) :</label>
                    <input type="text" class="form-control form-control-border" 
                    id="exampleInputBorder" value="<%= penempatan.supervisor.nip %>" disabled>
                  </div>
                  <div class="form-group">
                    <label for="exampleInputBorder">Jabatan Pembimbing lapangan (Supervisor) :</label>
                    <input type="text" class="form-control form-control-border" 
                    id="exampleInputBorder" value="<%= penempatan.supervisor.jabatan %>" disabled>
                  </div>
                  <hr>

                  <div class="form-group">
                    <label for="exampleInputBorder">Bidang Penempatan :</label>
                    <input type="text" class="form-control form-control-border" 
                    id="exampleInputBorder" value="<%= penempatan.biro.nama %> " disabled>
                  </div>
                  <div class="row">
                    <div class="col">
                      <div class="form-group">
                        <label for="exampleInputBorder">Tanggal mulai penempatan magang :</label>
                        <input type="text" class="form-control form-control-border" 
                        id="exampleInputBorder" value="<%= tglFormatForm(penempatan.tglmulai_penempatan) %>" disabled>
                      </div>
                    </div>
                    <div class="col">
                      <div class="form-group">
                        <label for="exampleInputBorder">Tanggal selesai penempatan magang :</label>
                        <input type="text" class="form-control form-control-border" 
                        id="exampleInputBorder" value="<%= tglFormatForm(penempatan.tglselesai_penempatan) %>" disabled>
                      </div>
                    </div>
                  </div>
                </div>

                <div class="card-footer">
                  <a href="/penempatan" type="submit" class="btn btn-primary">Kembali</a>
                </div>
            </div>
            <!-- /.card -->
          </div>
          <!-- /.card -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
  </section>
  <!-- /.content -->
</div>
<!-- /.content-wrapper -->

<%- include ../../partials/footer.ejs %>
<%- include ../../partials/js.ejs %>